{
  "env": {
  "browser": true,
      "es6": true,
      "node": true,
      "jest/globals": true
},
  "extends": [
  "plugin:react/recommended",
  "standard"
],
    "globals": {
  "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
},
  "parserOptions": {
  "ecmaFeatures": {
    "jsx": true
  },
  "ecmaVersion": 11,
      "sourceType": "module"
},
  "plugins": [
  "react",
  "jest"
],
    "rules": {
  "react/prop-types": 0,
      "indent": ["error", 4],
      "jest/no-disabled-tests": "warn",
      "jest/no-focused-tests": "error",
      "jest/no-identical-title": "error",
      "jest/prefer-to-have-length": "warn",
      "jest/valid-expect": "error",
      "no-extra-semi": "error",
      "semi": "error"


}
}
