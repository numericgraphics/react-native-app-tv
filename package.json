{
  "name": "rts-app-tv",
  "version": "0.0.23",
  "description": "RTS TV App Framework",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "bitbucket",
    "url": "https://bitbucket.org:rtsmb/rts-app-tv.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "react-app-rewired start",
    "web:build": "react-app-rewired build",
    "test": "jest",
    "lint": "eslint .",
    "clearAll": "watchman watch-del-all && rm -rf node_modules && rm yarn.lock && yarn cache clean && yarn install && rm -rf /tmp/metro-* && rm -fr $TMPDIR/metro-cache",
    "dist": "npx babel src/lib --out-dir dist",
    "docs": "jsdoc -c jsdoc.json -t docs_src/template/default/"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "npm:react-native-tvos@0.63.4-0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-web": "0.16.3",
    "react-native-web-linear-gradient": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.14.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-plugin-react-native-web": "^0.15.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.2",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
