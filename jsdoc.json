{
  "source": {
    "include": ["./docs_src/components"],
    "includePattern": ".(jsx|js|ts|tsx)$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "search": true,
    "default": {
      "outputSourceFiles": false
    }
  }
}




